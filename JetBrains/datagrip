#!/bin/bash
#Generated by JetBrains Toolbox 1.27.2.13801 at 2023-01-27T14:53:39.930570

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/bstiles/Library/Application Support/JetBrains/Toolbox/apps/datagrip/ch-0/223.8617.3/DataGrip.app/Contents/MacOS/datagrip" $wait --args "${ideargs[@]}"